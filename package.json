{
    "name": "tekena-reloaded",
    "version": "1.0.0",
    "description": "Papa Tekena con Esteroides",
    "private": true,
    "scripts": {
        "lint": "tslint src/**/**/*.ts --project tsconfig.json --config tslint.json",
        "clean:dist:main": "rm -rf ./wwwroot/dist/*",
        "clean:dist:submodules": "rm -rf ./dist_submodules/*",
        "serve": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --history-api-fallback --content-base wwwroot/ --hot",
        "build:dev": "yarn run clean:dist:main && yarn run clean:dist:submodules && yarn run build:dev:main && yarn run build:dev:submodules",
        "build:dev:main": "./node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js --env.dev && webpack --env.dev",
        "build:dev:submodules": "./node_modules/webpack/bin/webpack.js --config webpack.config.submodules.js --env.dev && webpack --env.dev",
        "build:prod": "yarn run clean:dist:main && yarn run clean:dist:submodules && yarn run build:prod:main && yarn run build:prod:submodules",
        "build:prod:main": "./node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js --env.prod && webpack --env.prod",
        "build:prod:submodules": "./node_modules/webpack/bin/webpack.js --config webpack.config.submodules.js --env.prod && webpack --env.prod"
    },
    "dependencies": {
        "aspnet-webpack": "^2.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@types/webpack-env": "1.13.6",
        "awesome-typescript-loader": "4.0.0",
        "axios": "0.18.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.0.5",
        "babel-preset-env": "^1.7.0",
        "babel-runtime": "^6.26.0",
        "css-loader": "0.25.0",
        "event-source-polyfill": "0.0.12",
        "express": "^4.16.4",
        "extract-text-webpack-plugin": "3.0.2",
        "file-loader": "^0.9.0",
        "file-saver": "^1.3.8",
        "generate-password": "^1.4.0",
        "material-design-icons-iconfont": "^4.0.2",
        "moment": "2.22.2",
        "node-sass": "^4.9.2",
        "prettier": "1.14.0",
        "sass-loader": "7.0.3",
        "style-loader": "0.21.0",
        "tslint": "5.11.0",
        "tslint-config-standard": "7.1.0",
        "types-vue": "1.0.10",
        "typescript": "2.9.2",
        "url-loader": "^0.5.7",
        "vue": "2.5.16",
        "vue-codemirror": "^4.0.6",
        "vue-color": "2.4.6",
        "vue-loader": "11.3.4",
        "vue-markdown": "^2.2.4",
        "vue-router": "3.0.1",
        "vue-template-compiler": "2.5.16",
        "vuetify": "1.1.13",
        "vuex": "3.0.1",
        "webpack": "3.1.0",
        "webpack-cli": "3.1.0",
        "webpack-dev-server": "2.9.1",
        "webpack-hot-middleware": "2.22.3"
    }
}
